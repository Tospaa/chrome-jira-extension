{
  "name": "Jira Quick Access",
  "description": "Opens the Jira ticket page of the selected Jira key",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "notifications"
  ],
  "background": {
    "service_worker": "bg-wrapper.js"
  },
  "manifest_version": 3,
  "icons": {
    "16": "/icons/16.png",
    "32": "/icons/32.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  }
}
